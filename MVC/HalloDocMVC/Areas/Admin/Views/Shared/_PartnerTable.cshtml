<div class="table-responsive">
    <table class="table">
        <thead>
            <th>Profession</th>
            <th>Business Name</th>
            <th>Email</th>
            <th>Fax Number</th>
            <th>Phone Number</th>
            <th>Business Contact</th>
            <th>Action</th>
        </thead>
        <tbody>
            @foreach(var item in Model.VendorsDetails){
                <tr>
                    <td>@item.ProfessionName</td>
                    <td>@item.BusinessName</td>
                    <td>@item.Email</td>
                    <td>@item.FaxNumber</td>
                    <td>@item.PhoneNumber</td>
                    <td>@item.BusinessContact</td>
                    <td>
                        <div class="d-flex gap-2">
                            <a class="btn theme-btn" asp-area="Admin" asp-controller="Partner" asp-action="Edit" asp-route-id="@item.Id">Edit</a>
                            <button onclick="deleteConfirmModal('@item.Id')" class="btn theme-btn">Delete</button>
                        </div>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>